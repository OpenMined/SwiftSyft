{
  "name": "SwiftSyft",
  "version": "0.1.0",
  "summary": "The official Syft worker for iOS, built in Swift.",
  "description": "SwiftSyft allows developers to integrate their apps as a worker to PySyft to facilitate\nFederated Learning.",
  "homepage": "https://github.com/OpenMined/SwiftSyft",
  "license": {
    "type": "Apache 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "OpenMined": "author@openmined.com"
  },
  "source": {
    "git": "https://github.com/OpenMined/SwiftSyft.git",
    "tag": "0.1.0"
  },
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": "5.1.3",
  "source_files": "SwiftSyft/**/*",
  "static_framework": true,
  "dependencies": {
    "LibTorch": [
      "~> 1.3.0"
    ],
    "GoogleWebRTC": [
      "~> 1.1.0"
    ]
  },
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/*.swift"
    }
  ],
  "swift_version": "5.1.3"
}
